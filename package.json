{
  "name": "drgn",
  "version": "0.0.0",
  "private": true,
  "main": "out/api.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:api\" \"npm run dev:pages\" \"nw src/\"",
    "dev:pages": "next dev",
    "dev:api": "concurrently --kill-others \"esbuild src/api/index.ts --bundle --watch --minify --platform=node --outfile=out/api.js\" \"nodemon out/api.js\"",
    "build": "npm run export && esbuild src/api/index.ts --bundle --minify --platform=node --outfile=out/api.js",
    "export": "next build && next export",
    "package": "nwbuild --platforms win32,win64,osx64,linux32,linux64 --buildDir packages/ src/"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.3",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@tippyjs/react": "^4.2.6",
    "dayjs": "^1.10.8",
    "fastify": "^3.27.4",
    "fs-extra": "^10.0.1",
    "material-icons": "^1.10.8",
    "next": "12.1.0",
    "node-fetch": "^2.6.7",
    "node-ssh": "^12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.25",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "nodemon": "^2.0.15",
    "nw": "^0.62.0",
    "nw-builder": "^3.5.7",
    "typescript": "4.6.2"
  }
}
