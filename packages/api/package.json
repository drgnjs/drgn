{
  "name": "@drgn/api",
  "version": "1.1.12",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "build:watch": "node scripts/watch.js",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"nodemon dist/index.js\"",
    "lint": "eslint src --ext .ts",
    "format": "eslint src --fix",
    "pre:version": "npm run format",
    "version:canary": "npm run pre:version && npm version prerelease --preid=canary",
    "version:patch": "npm run pre:version && npm version patch",
    "version:minor": "npm run pre:version && npm version minor",
    "version:major": "npm run pre:version && npm version major",
    "deploy": "node scripts/deploy.js",
    "types": "tsc -w"
  },
  "dependencies": {
    "@fastify/compress": "^6.1.0",
    "@fastify/cors": "^8.0.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/jwt": "^6.2.1",
    "@fastify/rate-limit": "^7.0.0",
    "@fastify/type-provider-typebox": "^2.1.0",
    "@sinclair/typebox": "^0.24.16",
    "cachu": "^5.4.4",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "fastify": "^4.2.0",
    "mongoose": "^6.4.4",
    "ms": "^2.1.3",
    "nanoid": "^4.0.0",
    "node-ssh": "^12.0.4",
    "nodemailer": "^6.7.7",
    "otplib": "^12.0.1"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.3",
    "@types/ssh2": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  }
}
