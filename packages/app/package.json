{
  "name": "@drgn/app",
  "version": "1.1.5",
  "description": "A better SSH Client",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"tauri dev\"",
    "dev:api": "cd ../../code/!/drgn-api && npm run dev",
    "pre:version": "npm run format",
    "version:canary": "npm run pre:version && npm version prerelease --preid=canary",
    "version:patch": "npm run pre:version && npm version patch",
    "version:minor": "npm run pre:version && npm version minor",
    "version:major": "npm run pre:version && npm version major",
    "build": "vite build",
    "bundle": "tauri build",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "eslint src --fix"
  },
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/azury"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drgnjs/drgn.git"
  },
  "keywords": [
    "ssh",
    "ssh-client",
    "desktop",
    "drgn",
    "typescript",
    "cross-platform",
    "windows",
    "linux",
    "mac"
  ],
  "author": "Samuel Kopp <hello@azury.dev> (https://azury.dev)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/drgnjs/drgn/issues"
  },
  "homepage": "https://github.com/drgnjs/drgn#readme",
  "dependencies": {
    "@dicebear/avatars": "^4.10.3",
    "@dicebear/avatars-identicon-sprites": "^4.10.3",
    "@fontsource/poppins": "^4.5.8",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@tauri-apps/api": "^1.0.2",
    "@tippyjs/react": "^4.2.6",
    "dayjs": "^1.11.3",
    "material-symbols": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "sass": "^1.54.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14"
  },
  "type": "module"
}
